                    <GradientStop Color="#FF534F4F" Offset="1" />
                    <GradientStop Color="#FF5B5B5B" Offset="0.387" />
                    <GradientStop Color="#FF5F5F5F" Offset="0.708" />
                </LinearGradientBrush>
            </Grid.Background>
            <StackPanel>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" HorizontalAlignment="Left">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0">
                            <Label Content="{Binding Language.NumberClient}" Foreground="White" Padding="10,3,0,3" FontSize="11" />
                            <Label Content="{Binding GameM.Clients,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Foreground="White" Padding="2,3,0,3" FontSize="11" />
                            <Label Content="/"  Foreground="White" Padding="2,3,0,3" FontSize="11" />
                            <Label Content="{Binding GameVM.LoginM.RegClient, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Foreground="White" Padding="2,3,0,3" FontSize="11" />
                            <Label Content="{Binding Language.Limit_Crep}" Foreground="White" Padding="10,3,0,3" FontSize="11" />
                            <Label Content="{Binding GameVM.LoginM.Max_limit_crep, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Foreground="White" Padding="2,3,0,2" FontSize="11" />
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Column="1" Margin="32,1,0,1" HorizontalAlignment="Right">
                        <StackPanel Orientation="Horizontal" Height="17"  Width="83" Margin="0,0,10,0">
                            <Label Content="{Binding Language.Version}" Foreground="White" Padding="0,2" FontSize="11" />
                            <Label Content="{Binding GameVM.InfoV.Version, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Foreground="White" Padding="2,2,0,2" FontSize="11" />
                        </StackPanel>
                    </Grid>
                </Grid>
            </StackPanel>
        </Grid>
    </Grid>
</Window>